{
  "datasets": [
    {
      "name": "hpx_first_use_behavior_flow_dataset",
      "displayName": "HPX First Use Behavior Flow",
      "asset_name": "workspace.default.mv_hpx_first_use_behavior_flow"
    }
  ],
  "pages": [
    {
      "name": "hpx_first_use_behavior_flow_page",
      "displayName": "HPX First Use Behavior Flow",
      "layout": [
        {
          "widget": {
            "name": "first_use_behavior_flow_sankey",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_first_use_behavior_flow_dataset",
                  "fields": [
                    {
                      "name": "Screen 1",
                      "expression": "`Screen 1`"
                    },
                    {
                      "name": "Screen 2",
                      "expression": "`Screen 2`"
                    },
                    {
                      "name": "Screen 3",
                      "expression": "`Screen 3`"
                    },
                    {
                      "name": "Screen 4",
                      "expression": "`Screen 4`"
                    },
                    {
                      "name": "Screen 5",
                      "expression": "`Screen 5`"
                    },
                    {
                      "name": "Total Devices",
                      "expression": "MEASURE(`Total Devices`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 1,
              "widgetType": "sankey",
              "encodings": {
                "value": {
                  "fieldName": "Total Devices",
                  "displayName": "Total Devices"
                },
                "stages": [
                  {
                    "fieldName": "Screen 1",
                    "displayName": "Screen 1"
                  },
                  {
                    "fieldName": "Screen 2",
                    "displayName": "Screen 2"
                  },
                  {
                    "fieldName": "Screen 3",
                    "displayName": "Screen 3"
                  },
                  {
                    "fieldName": "Screen 4",
                    "displayName": "Screen 4"
                  },
                  {
                    "fieldName": "Screen 5",
                    "displayName": "Screen 5"
                  }
                ]
              },
              "frame": {
                "title": "HPX First Use Behavior Flow",
                "showTitle": true,
                "subtitle": "Sankey Diagram as user moves between Screens 1 - 5"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "Date",
          "displayName": "Date",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "App Version",
          "displayName": "App Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}