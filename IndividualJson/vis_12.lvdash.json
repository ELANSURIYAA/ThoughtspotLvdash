{
  "datasets": [
    {
      "name": "hpx_curr_dataset",
      "displayName": "HPX CURR Dataset",
      "asset_name": "workspace.default.mv_hpx_curr"
    }
  ],
  "pages": [
    {
      "name": "hpx_curr_page",
      "displayName": "HPX CURR - Data Table",
      "layout": [
        {
          "widget": {
            "name": "curr_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_curr_dataset",
                  "fields": [
                    {
                      "name": "Week",
                      "expression": "`Weekly Week Active`"
                    },
                    {
                      "name": "CURR",
                      "expression": "MEASURE(`Curr`)"
                    },
                    {
                      "name": "Previous Week Users T14t20",
                      "expression": "MEASURE(`Total Previous Week Users T14t20`)"
                    },
                    {
                      "name": "Last Week Users T7t13",
                      "expression": "MEASURE(`Total Last Week Users T7t13`)"
                    },
                    {
                      "name": "This Week Users T0t6",
                      "expression": "MEASURE(`Total This Week Users T0t6`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "Week",
                  "displayName": "Week",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "CURR",
                      "displayName": "CURR"
                    },
                    {
                      "fieldName": "Previous Week Users T14t20",
                      "displayName": "Previous Week Users T14t20"
                    },
                    {
                      "fieldName": "Last Week Users T7t13",
                      "displayName": "Last Week Users T7t13"
                    },
                    {
                      "fieldName": "This Week Users T0t6",
                      "displayName": "This Week Users T0t6"
                    }
                  ],
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "title": "HPX CURR - % Devices active in different time periods",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}