{
  "datasets": [
    {
      "name": "dataset_1",
      "displayName": "Active Devices Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Active Devices Visualization",
      "layout": [
        {
          "widget": {
            "name": "widget_1",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_1",
                  "fields": [
                    {
                      "name": "daily_event_date",
                      "expression": "DATE_TRUNC('DAY', event_date_time)"
                    },
                    {
                      "name": "active_devices",
                      "expression": "COUNT(DISTINCT sys_sn_hash)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Active Devices Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}