{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "asset_name": "workspace.default.mv_hpx_retention"
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    {
                      "name": "monthly_first_device_event",
                      "expression": "`Monthly First Device Event Date Time`"
                    },
                    {
                      "name": "months_since_first_use",
                      "expression": "`Months Since First Use`"
                    },
                    {
                      "name": "session_type",
                      "expression": "`Session Type`"
                    },
                    {
                      "name": "active_devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "percent_devices_returning",
                      "expression": "MEASURE(`Active Devices`) / WINDOW_AGGREGATE(MEASURE(`Active Devices`), PARTITION BY `Monthly First Device Event Date Time`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivot-table",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "monthly_first_device_event",
                    "displayName": "Month(First Device Event Date Time)",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "session_type",
                    "displayName": "Session Type",
                    "scale": { "type": "categorical" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "percent_devices_returning",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" }
                  }
                ]
              },
              "frame": {
                "title": "0.5 HPX Retention - Cohort Graph",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}