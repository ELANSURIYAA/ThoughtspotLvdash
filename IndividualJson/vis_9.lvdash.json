{
  "datasets": [
    {
      "name": "dataset_9",
      "displayName": "HPX Retention",
      "asset_name": "workspace.default.mv_hpx_retention"
    }
  ],
  "pages": [
    {
      "name": "page_9",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "widget_9",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_9",
                  "fields": [
                    {
                      "name": "Month(First Device Event Date Time)",
                      "expression": "DATE_TRUNC('MONTH', first_device_event_date_time)"
                    },
                    {
                      "name": "Months Since First Use",
                      "expression": "Months Since First Use"
                    },
                    {
                      "name": "Session Type",
                      "expression": "Session Type"
                    },
                    {
                      "name": "Weeks Since First Use",
                      "expression": "Weeks Since First Use"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(Active Devices)"
                    },
                    {
                      "name": "% Devices Returning",
                      "expression": "MEASURE(Active Devices) / SUM(MEASURE(Active Devices)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time))"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivot_table",
              "encodings": {
                "x": {
                  "fieldName": "Month(First Device Event Date Time)",
                  "displayName": "Month(First Device Event Date Time)"
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "% Devices Returning",
                      "displayName": "% Devices Returning"
                    },
                    {
                      "fieldName": "Active Devices",
                      "displayName": "Active Devices"
                    }
                  ]
                },
                "color": {
                  "fields": [
                    {
                      "fieldName": "Months Since First Use",
                      "displayName": "Months Since First Use"
                    },
                    {
                      "fieldName": "Session Type",
                      "displayName": "Session Type"
                    }
                  ]
                }
              },
              "frame": {
                "showTitle": true,
                "title": "HPX Retention Cohort Analysis"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}