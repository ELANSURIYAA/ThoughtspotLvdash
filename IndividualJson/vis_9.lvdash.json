{
    "datasets": [
      {
        "name": "dataset_hpx_retention",
        "displayName": "HPX Retention Dataset",
        "asset_name": "workspace.default.mv_hpx_retention"
      }
    ],
    "pages": [
      {
        "name": "page_9",
        "displayName": "HPX Retention Visualization",
        "layout": [
          {
            "widget": {
              "name": "pivot_table_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_hpx_retention",
                    "fields": [
                      { "name": "Month(First Device Event Date Time)", "expression": "DATE_TRUNC('MONTH', first_device_event_date_time)" },
                      { "name": "% Devices Returning", "expression": "MEASURE(% Devices Returning)" },
                      { "name": "Months Since First Use", "expression": "months_since_first_use" },
                      { "name": "Session Type", "expression": "session_type" },
                      { "name": "Weeks Since First Use", "expression": "weeks_since_first_use" },
                      { "name": "Active Devices", "expression": "MEASURE(Active Devices)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "pivot_table",
                "encodings": {
                  "x": {
                    "fieldName": "Month(First Device Event Date Time)",
                    "displayName": "Month(First Device Event Date Time)",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "% Devices Returning",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fields": [
                      {
                        "fieldName": "Months Since First Use",
                        "displayName": "Months Since First Use"
                      },
                      {
                        "fieldName": "Session Type",
                        "displayName": "Session Type"
                      }
                    ],
                    "scale": { "type": "categorical" }
                  }
                },
                "frame": {
                  "title": "HPX Retention Cohort Analysis",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }