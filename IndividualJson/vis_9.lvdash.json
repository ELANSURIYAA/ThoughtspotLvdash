{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention",
      "asset_name": "workspace.default.mv_hpx_retention"
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    {
                      "name": "Month(First Device Event Date Time)",
                      "expression": "`Monthly First Device Event Date Time`"
                    },
                    {
                      "name": "Months Since First Use",
                      "expression": "`Months Since First Use`"
                    },
                    {
                      "name": "Session Type",
                      "expression": "`Session Type`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "% Devices Returning",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER (PARTITION BY `Monthly First Device Event Date Time`)"
                    }
                  ],
                  "disaggregated": false,
                  "filters": [
                    {
                      "name": "date_filter",
                      "expression": "`First Device Event Date Time` >= DATE '2023-05-01'"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "Month(First Device Event Date Time)",
                    "displayName": "Month(First Device Event Date Time)"
                  }
                ],
                "columns": [
                  {
                    "fieldName": "Months Since First Use",
                    "displayName": "Months Since First Use"
                  },
                  {
                    "fieldName": "Session Type",
                    "displayName": "Session Type"
                  }
                ],
                "values": [
                  {
                    "fieldName": "% Devices Returning",
                    "displayName": "% Devices Returning",
                    "format": {
                      "type": "percent",
                      "decimalPlaces": 1
                    }
                  }
                ]
              },
              "frame": {
                "showTitle": true,
                "title": "0.5 HPX Retention"
              },
              "options": {
                "heatmap": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}