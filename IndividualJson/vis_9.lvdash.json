{
    "datasets": [
      {
        "name": "dataset_9",
        "displayName": "HPX Retention Dataset",
        "asset_name": "workspace.default.HPX_Retention_CSS_Analytics"
      }
    ],
    "pages": [
      {
        "name": "page_9",
        "displayName": "HPX Retention Overview",
        "layout": [
          {
            "widget": {
              "name": "pivot_table_widget_9",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_9",
                    "fields": [
                      { "name": "Month(First Device Event Date Time)", "expression": "DATE_TRUNC('MONTH', `First Device Event Date Time`)" },
                      { "name": "% Devices Returning", "expression": "MEASURE(`% Devices Returning`)" },
                      { "name": "Active Devices", "expression": "MEASURE(`Active_Devices`)" },
                      { "name": "Months Since First Use", "expression": "`Months Since First Use`" },
                      { "name": "Session Type", "expression": "`Session Type`" },
                      { "name": "Weeks Since First Use", "expression": "`Weeks Since First Use`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "pivot_table",
                "encodings": {
                  "row": {
                    "fieldName": "Month(First Device Event Date Time)",
                    "displayName": "Month",
                    "scale": { "type": "temporal" }
                  },
                  "column": {
                    "fieldName": "Months Since First Use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "categorical" }
                  },
                  "value": {
                    "fieldName": "% Devices Returning",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "HPX Retention Overview",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }