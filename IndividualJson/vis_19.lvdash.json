{
  "datasets": [
    {
      "name": "homepage_summary_dataset",
      "displayName": "Homepage Summary Dataset",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "home_tile_secondary_ctr_page",
      "displayName": "Home Tile Secondary CTR",
      "layout": [
        {
          "widget": {
            "name": "secondary_ctr_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "homepage_summary_dataset",
                  "fields": [
                    {
                      "name": "Home Tile",
                      "expression": "`Home Tile`"
                    },
                    {
                      "name": "Secondary Button CTR",
                      "expression": "MEASURE(`Total Unique Interstitial Secondary Button Clicks`) / MEASURE(`Total Unique Interstitial Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "Secondary Button CTR",
                  "displayName": "% Secondary Button Clicked Vs Interstitial Displayed",
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  },
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "category": {
                  "fieldName": "Home Tile",
                  "displayName": "Home Tile",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Home Tile Secondary CTR"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}