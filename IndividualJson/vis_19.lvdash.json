{
  "datasets": [
    {
      "name": "hpx_homepage_summary_dataset",
      "displayName": "HPX Homepage Summary Dataset",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "home_tile_secondary_ctr_page",
      "displayName": "Home Tile Secondary CTR",
      "layout": [
        {
          "widget": {
            "name": "home_tile_secondary_ctr_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_homepage_summary_dataset",
                  "fields": [
                    {
                      "name": "home_tile",
                      "expression": "`Home Tile`"
                    },
                    {
                      "name": "unique_tile_views",
                      "expression": "MEASURE(`Total Unique Tile Views`)"
                    },
                    {
                      "name": "unique_tile_clicks",
                      "expression": "MEASURE(`Total Unique Tile Clicks`)"
                    },
                    {
                      "name": "unique_interstitial_views",
                      "expression": "MEASURE(`Total Unique Interstitial Views`)"
                    },
                    {
                      "name": "unique_interstitial_secondary_clicks",
                      "expression": "MEASURE(`Total Unique Interstitial Secondary Button Clicks`)"
                    },
                    {
                      "name": "secondary_ctr",
                      "expression": "MEASURE(`Total Unique Interstitial Secondary Button Clicks`) / MEASURE(`Total Unique Interstitial Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "secondary_ctr",
                  "displayName": "Secondary Button CTR",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Home Tile Secondary CTR"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 4
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}