{
    "datasets": [
      {
        "name": "dataset_19",
        "displayName": "HPX Events Model Homepage Summary Dataset",
        "asset_name": "workspace.default.mv_hpx_homepage_summary"
      }
    ],
    "pages": [
      {
        "name": "page_19",
        "displayName": "Home Tile Secondary CTR",
        "layout": [
          {
            "widget": {
              "name": "counter_widget_19",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_19",
                    "fields": [
                      { "name": "Home Tile", "expression": "home_tile" },
                      { "name": "% Secondary Button Clicked vs Interstitial Displayed", "expression": "MEASURE("Total Secondary Button Clicked Vs Interstitial Displayed")" },
                      { "name": "% Secondary Button Clicked vs Tile Displayed", "expression": "MEASURE("Total Secondary Button Clicked Vs Tile Displayed")" },
                      { "name": "Secondary CTR", "expression": "MEASURE("f_ctr_cta0")" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fieldName": "Secondary CTR",
                    "displayName": "Secondary CTR"
                  }
                },
                "frame": {
                  "title": "Home Tile Secondary CTR",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }