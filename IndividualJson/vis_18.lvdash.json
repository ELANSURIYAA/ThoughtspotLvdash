{
  "datasets": [
    {
      "name": "homepage_summary_dataset",
      "displayName": "Home Tile Primary CTR",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "home_tile_primary_ctr_page",
      "displayName": "Home Tile Primary CTR",
      "layout": [
        {
          "widget": {
            "name": "home_tile_primary_ctr_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "homepage_summary_dataset",
                  "fields": [
                    {
                      "name": "Home Tile",
                      "expression": "`Home Tile`"
                    },
                    {
                      "name": "Unique Tile Views",
                      "expression": "MEASURE(`Total Unique Tile Views`)"
                    },
                    {
                      "name": "Unique Tile Clicks",
                      "expression": "MEASURE(`Total Unique Tile Clicks`)"
                    },
                    {
                      "name": "Unique Interstitial Views",
                      "expression": "MEASURE(`Total Unique Interstitial Views`)"
                    },
                    {
                      "name": "Unique Interstitial Primary Button Clicks",
                      "expression": "MEASURE(`Total Unique Interstitial Primary Button Clicks`)"
                    },
                    {
                      "name": "CTR",
                      "expression": "MEASURE(`Total Unique Tile Clicks`) / MEASURE(`Total Unique Tile Views`)"
                    },
                    {
                      "name": "Primary CTR",
                      "expression": "MEASURE(`Total Unique Interstitial Primary Button Clicks`) / MEASURE(`Total Unique Interstitial Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "values": [
                  {
                    "fieldName": "Primary CTR",
                    "displayName": "Primary Button CTR",
                    "numberFormat": {
                      "unit": "PERCENT",
                      "pattern": "#,##0.0%"
                    }
                  },
                  {
                    "fieldName": "CTR",
                    "displayName": "Tile CTR",
                    "numberFormat": {
                      "unit": "PERCENT",
                      "pattern": "#,##0.0%"
                    }
                  },
                  {
                    "fieldName": "Unique Tile Views",
                    "displayName": "Unique Tile Views"
                  },
                  {
                    "fieldName": "Unique Tile Clicks",
                    "displayName": "Unique Tile Clicks"
                  },
                  {
                    "fieldName": "Unique Interstitial Views",
                    "displayName": "Unique Interstitial Views"
                  },
                  {
                    "fieldName": "Unique Interstitial Primary Button Clicks",
                    "displayName": "Unique Interstitial Primary Button Clicks"
                  }
                ]
              },
              "frame": {
                "showTitle": true,
                "title": "Home Tile Primary CTR"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}