{
    "datasets": [
      {
        "name": "dataset_18",
        "displayName": "Home Tile Primary CTR Dataset",
        "asset_name": "workspace.default.hpx_events_model_homepage_summary"
      }
    ],
    "pages": [
      {
        "name": "page_18",
        "displayName": "Home Tile Primary CTR Overview",
        "layout": [
          {
            "widget": {
              "name": "kpi_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_18",
                    "fields": [
                      { "name": "Home Tile", "expression": "`Home Tile`" },
                      { "name": "Primary CTR", "expression": "MEASURE(`f_ctr_cta1`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fieldName": "Primary CTR",
                    "displayName": "Primary CTR"
                  }
                },
                "frame": {
                  "title": "Home Tile Primary CTR",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }