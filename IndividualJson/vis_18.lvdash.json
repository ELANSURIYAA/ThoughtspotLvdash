{
    "datasets": [
      {
        "name": "dataset_18",
        "displayName": "HPX Events Model Homepage Summary",
        "asset_name": "workspace.default.mv_hpx_homepage_summary"
      }
    ],
    "pages": [
      {
        "name": "page_18",
        "displayName": "Home Tile Primary CTR",
        "layout": [
          {
            "widget": {
              "name": "widget_18",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_18",
                    "fields": [
                      { "name": "Home Tile", "expression": "home_tile" },
                      { "name": "% Home Tile Clicked Vs Homepage Displayed", "expression": "MEASURE(Total Home Tile Clicked Vs Homepage Displayed)" },
                      { "name": "% Primary Button Clicked Vs Interstitial Displayed", "expression": "MEASURE(Total Primary Button Clicked Vs Interstitial Displayed)" },
                      { "name": "% Primary Button Clicked Vs Tile Displayed", "expression": "MEASURE(Total Primary Button Clicked Vs Tile Displayed)" },
                      { "name": "% Secondary Button Clicked Vs Interstitial Displayed", "expression": "MEASURE(Total Secondary Button Clicked Vs Interstitial Displayed)" },
                      { "name": "% Secondary Button Clicked Vs Tile Displayed", "expression": "MEASURE(Total Secondary Button Clicked Vs Tile Displayed)" },
                      { "name": "Unique Interstitial Primary Button Clicks", "expression": "SUM(MEASURE(Total Unique Interstitial Primary Button Clicks))" },
                      { "name": "Unique Interstitial Secondary Button Clicks", "expression": "SUM(MEASURE(Total Unique Interstitial Secondary Button Clicks))" },
                      { "name": "Unique Interstitial Views", "expression": "SUM(MEASURE(Total Unique Interstitial Views))" },
                      { "name": "Unique Tile Clicks", "expression": "SUM(MEASURE(Total Unique Tile Clicks))" },
                      { "name": "Unique Tile Views", "expression": "SUM(MEASURE(Total Unique Tile Views))" },
                      { "name": "CTR", "expression": "MEASURE(f_ctr)" },
                      { "name": "Primary CTR", "expression": "MEASURE(f_ctr_cta1)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fields": [
                      { "fieldName": "Primary CTR", "displayName": "Primary CTR" },
                      { "fieldName": "CTR", "displayName": "CTR" },
                      { "fieldName": "Unique Tile Views", "displayName": "Unique Tile Views" },
                      { "fieldName": "Unique Tile Clicks", "displayName": "Unique Tile Clicks" },
                      { "fieldName": "Unique Interstitial Views", "displayName": "Unique Interstitial Views" },
                      { "fieldName": "Unique Interstitial Primary Button Clicks", "displayName": "Unique Interstitial Primary Button Clicks" },
                      { "fieldName": "Unique Interstitial Secondary Button Clicks", "displayName": "Unique Interstitial Secondary Button Clicks" }
                    ]
                  }
                },
                "frame": {
                  "title": "Home Tile Primary CTR",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
