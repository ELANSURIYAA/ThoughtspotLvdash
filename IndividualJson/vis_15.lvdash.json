{
    "datasets": [
      {
        "name": "dataset_15",
        "displayName": "Home Tile Views Dataset",
        "asset_name": "workspace.default.hpx_events_model_homepage_summary"
      }
    ],
    "pages": [
      {
        "name": "page_15",
        "displayName": "Home Tile Views Overview",
        "layout": [
          {
            "widget": {
              "name": "kpi_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_15",
                    "fields": [
                      { "name": "Home Tile", "expression": "`Home Tile`" },
                      { "name": "Unique Tile Views", "expression": "MEASURE(`Total Unique Tile Views`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fieldName": "Unique Tile Views",
                    "displayName": "Unique Tile Views"
                  }
                },
                "frame": {
                  "title": "Home Tile Views",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }