{
  "datasets": [
    {
      "name": "dataset_viz_3",
      "displayName": "HPX Events Model Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_viz_3",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "pie_chart_widget_viz_3",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_viz_3",
                  "fields": [
                    { "name": "Geo Country Code", "expression": "Geo Country Code" },
                    { "name": "New Devices", "expression": "MEASURE(New Devices)" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "New Devices",
                  "displayName": "New Devices"
                },
                "color": {
                  "fieldName": "Geo Country Code",
                  "displayName": "Geo Country Code"
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}