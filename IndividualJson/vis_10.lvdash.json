{
    "datasets": [
      {
        "name": "dataset_hpx_events_model",
        "displayName": "HPX Events Model Dataset",
        "asset_name": "workspace.default.mv_hpx_events_model"
      }
    ],
    "pages": [
      {
        "name": "page_10",
        "displayName": "Total Consented Devices Visualization",
        "layout": [
          {
            "widget": {
              "name": "kpi_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_hpx_events_model",
                    "fields": [
                      { "name": "Active Devices", "expression": "MEASURE(Active Devices)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fieldName": "Active Devices",
                    "displayName": "Active Devices",
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "Total Consented Devices",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }