{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "total_consented_devices_page",
      "displayName": "Total Consented Devices",
      "layout": [
        {
          "widget": {
            "name": "total_consented_devices_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    }
                  ],
                  "disaggregated": false,
                  "filters": [
                    {
                      "name": "app_version_filter_1",
                      "expression": "`App Version` != 'not a store release'"
                    },
                    {
                      "name": "app_version_filter_2",
                      "expression": "`App Version` IS NOT NULL"
                    },
                    {
                      "name": "date_filter",
                      "expression": "`Event Date Time` >= DATE '2023-05-01'"
                    },
                    {
                      "name": "geo_country_code_filter",
                      "expression": "`Geo Country Code` IS NOT NULL"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "Active Devices",
                  "displayName": "Active Devices",
                  "format": {
                    "type": "number",
                    "decimalPlaces": 0,
                    "useThousandsSeparator": true
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Total Consented Devices"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 4
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}