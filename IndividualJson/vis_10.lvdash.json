{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "total_consented_devices_page",
      "displayName": "Total Consented Devices",
      "layout": [
        {
          "widget": {
            "name": "total_consented_devices_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Active_Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "Active_Devices",
                  "displayName": "Active Devices"
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Total Consented Devices"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 4
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}