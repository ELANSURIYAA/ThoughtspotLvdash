{
    "datasets": [
      {
        "name": "dataset_hpx_events_model",
        "displayName": "HPX Events Model Dataset",
        "asset_name": "workspace.default.mv_hpx_events_model"
      }
    ],
    "pages": [
      {
        "name": "page_3",
        "displayName": "New Devices by Top 10 Countries Visualization",
        "layout": [
          {
            "widget": {
              "name": "pie_chart_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_hpx_events_model",
                    "fields": [
                      { "name": "Geo Country Code", "expression": "geo_country_code" },
                      { "name": "New Devices", "expression": "SUM(MEASURE(New Devices))" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "pie",
                "encodings": {
                  "angle": {
                    "fieldName": "New Devices",
                    "displayName": "New Devices",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fieldName": "Geo Country Code",
                    "displayName": "Geo Country Code",
                    "scale": { "type": "categorical" }
                  }
                },
                "frame": {
                  "title": "New Devices by Top 10 Countries",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }