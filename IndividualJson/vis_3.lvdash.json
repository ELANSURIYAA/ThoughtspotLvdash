{
    "datasets": [
      {
        "name": "dataset_viz_3",
        "displayName": "New Devices by Country Dataset",
        "asset_name": "workspace.default.mv_hpx_events_model"
      }
    ],
    "pages": [
      {
        "name": "page_viz_3",
        "displayName": "New Devices by Top 10 Countries",
        "layout": [
          {
            "widget": {
              "name": "widget_viz_3",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_viz_3",
                    "fields": [
                      {
                        "name": "Geo Country Code",
                        "expression": "`Geo Country Code`"
                      },
                      {
                        "name": "New Devices",
                        "expression": "MEASURE(`New Devices`)"
                      },
                      {
                        "name": "Rank",
                        "expression": "rank(MEASURE(`New Devices`), 'DESC')"
                      }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 1,
                "widgetType": "sankey",
                "encodings": {
                  "value": {
                    "fieldName": "New Devices",
                    "displayName": "New Devices"
                  },
                  "stages": [
                    {
                      "fieldName": "Geo Country Code",
                      "displayName": "Country"
                    },
                    {
                      "fieldName": "Rank",
                      "displayName": "Rank"
                    }
                  ]
                }
              }
            },
            "position": {
              "x": 0,
              "y": 0,
              "width": 12,
              "height": 8
            }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }