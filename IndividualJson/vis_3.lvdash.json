{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "new_devices_by_country",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_by_country_pie",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Geo Country Code",
                      "expression": "`Geo Country Code`"
                    },
                    {
                      "name": "New Devices",
                      "expression": "MEASURE(`New Devices`)"
                    },
                    {
                      "name": "rank",
                      "expression": "RANK() OVER (ORDER BY MEASURE(`New Devices`) DESC)"
                    }
                  ],
                  "filters": [
                    {
                      "name": "top_countries",
                      "expression": "rank <= 10"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "color": {
                  "fieldName": "Geo Country Code",
                  "displayName": "Country",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "angle": {
                  "fieldName": "New Devices",
                  "displayName": "New Devices",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}