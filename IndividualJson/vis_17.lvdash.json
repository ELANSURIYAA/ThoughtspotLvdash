{
  "datasets": [
    {
      "name": "homepage_summary_dataset",
      "displayName": "Home Tile Interactions CTR",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "home_tile_interactions_page",
      "displayName": "Home Tile Interactions CTR",
      "layout": [
        {
          "widget": {
            "name": "home_tile_ctr_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "homepage_summary_dataset",
                  "fields": [
                    {
                      "name": "Home Tile",
                      "expression": "`Home Tile`"
                    },
                    {
                      "name": "Unique Tile Views",
                      "expression": "MEASURE(`Total Unique Tile Views`)"
                    },
                    {
                      "name": "Unique Tile Clicks",
                      "expression": "MEASURE(`Total Unique Tile Clicks`)"
                    },
                    {
                      "name": "CTR",
                      "expression": "MEASURE(`Total Unique Tile Clicks`) / MEASURE(`Total Unique Tile Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "values": [
                  {
                    "fieldName": "CTR",
                    "displayName": "Click-Through Rate",
                    "format": {
                      "type": "percent",
                      "decimalPlaces": 2
                    }
                  },
                  {
                    "fieldName": "Unique Tile Clicks",
                    "displayName": "Unique Tile Clicks"
                  },
                  {
                    "fieldName": "Unique Tile Views",
                    "displayName": "Unique Tile Views"
                  }
                ]
              },
              "frame": {
                "title": "Home Tile Interactions CTR",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}