{
    "datasets": [
      {
        "name": "dataset_17",
        "displayName": "Home Tile Interactions CTR Dataset",
        "asset_name": "workspace.default.mv_hpx_events_model_homepage_summary",
        "queryLines": [
          "SELECT Home_Tile, SUM(Unique_Tile_Clicks) / SUM(Unique_Tile_Views) AS CTR FROM hpx_events_model_homepage_summary GROUP BY Home_Tile ORDER BY CTR DESC"
        ]
      }
    ],
    "pages": [
      {
        "name": "page_17",
        "displayName": "Home Tile Interactions CTR Visualization",
        "layout": [
          {
            "widget": {
              "name": "widget_17",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_17",
                    "fields": [
                      { "name": "Home_Tile", "expression": "`Home_Tile`" },
                      { "name": "CTR", "expression": "`CTR`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "value": {
                    "fieldName": "CTR",
                    "displayName": "Click Through Rate (CTR)"
                  }
                },
                "frame": {
                  "title": "Home Tile Interactions CTR",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }