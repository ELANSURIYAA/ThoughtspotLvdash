{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "country_analysis_page",
      "displayName": "Country Analysis",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Geo Country Code",
                      "expression": "`Geo Country Code`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "Percent Active Devices",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER ()"
                    }
                  ],
                  "disaggregated": false,
                  "orderBy": [
                    {
                      "fieldName": "Percent Active Devices",
                      "direction": "DESC"
                    }
                  ],
                  "limit": 10
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "Geo Country Code",
                  "displayName": "Country",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "y": {
                  "fieldName": "Percent Active Devices",
                  "displayName": "% Active Devices by Country",
                  "scale": {
                    "type": "quantitative"
                  },
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "% Active Devices by Country (Top 10)"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}