{
  "datasets": [
    {
      "name": "dataset_active_devices_by_country",
      "displayName": "Active Devices by Country",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_country_page",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_active_devices_by_country",
                  "fields": [
                    {
                      "name": "Geo Country Code",
                      "expression": "`Geo Country Code`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "Percent Active Devices by Country",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER ()"
                    }
                  ],
                  "disaggregated": false,
                  "limit": 10,
                  "orderBy": [
                    {
                      "fieldName": "Percent Active Devices by Country",
                      "direction": "DESC"
                    }
                  ]
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "Geo Country Code",
                  "displayName": "Country",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "y": {
                  "fieldName": "Percent Active Devices by Country",
                  "displayName": "% Active Devices by Country",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "% Active Devices by Country (Top 10)"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "App Version",
          "displayName": "App Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "excludedValues": ["not a store release", "other non-releases"]
        },
        {
          "fieldName": "Event Date Time",
          "displayName": "Event Date Time",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "defaultValues": {
            "operator": "GREATER_THAN_OR_EQUAL_TO",
            "values": ["2023-05-01"]
          }
        },
        {
          "fieldName": "Os Version",
          "displayName": "Os Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "excludedValues": [null]
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}