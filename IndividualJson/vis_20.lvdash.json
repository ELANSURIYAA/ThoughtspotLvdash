{
  "datasets": [
    {
      "name": "dataset_active_devices_by_country",
      "displayName": "Active Devices by Country",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_country_page",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_active_devices_by_country",
                  "fields": [
                    {
                      "name": "Geo Country Code",
                      "expression": "`Geo Country Code`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "Total Active Devices",
                      "expression": "SUM(MEASURE(`Active Devices`)) OVER ()"
                    },
                    {
                      "name": "% Active Devices by Country",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER ()"
                    },
                    {
                      "name": "f_active_devices_rank",
                      "expression": "RANK() OVER (ORDER BY MEASURE(`Active Devices`) DESC)"
                    }
                  ],
                  "filters": [
                    {
                      "name": "rank_filter",
                      "expression": "f_active_devices_rank <= 10"
                    },
                    {
                      "name": "app_version_filter",
                      "expression": "`App Version` != 'not a store release' AND `App Version` != 'other non-releases'"
                    },
                    {
                      "name": "os_version_filter",
                      "expression": "`Os Version` IS NOT NULL"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "Geo Country Code",
                  "displayName": "Country",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "y": {
                  "fieldName": "% Active Devices by Country",
                  "displayName": "% Active Devices by Country",
                  "scale": {
                    "type": "quantitative"
                  },
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  }
                }
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}