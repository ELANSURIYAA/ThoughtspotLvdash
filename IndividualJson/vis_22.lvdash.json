{
    "datasets": [
      {
        "name": "dataset_22",
        "displayName": "HPX New Version Adoption Dataset",
        "asset_name": "workspace.default.mv_hpx_events_model",
        "queryLines": [
          "SELECT DATE_TRUNC('DAY', Event_Date_Time) AS Daily_Event_Date, App_Version, SUM(Active_Devices) AS Active_Devices, SUM(Active_Devices) / SUM(SUM(Active_Devices)) OVER (PARTITION BY App_Version ORDER BY DATE_TRUNC('DAY', Event_Date_Time)) AS Percentage_Active_Devices FROM hpx_events_model WHERE Event_Date_Time >= '2023-05-01' GROUP BY DATE_TRUNC('DAY', Event_Date_Time), App_Version ORDER BY Daily_Event_Date"
        ]
      }
    ],
    "pages": [
      {
        "name": "page_22",
        "displayName": "HPX New Version Adoption Visualization",
        "layout": [
          {
            "widget": {
              "name": "widget_22",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_22",
                    "fields": [
                      { "name": "Daily_Event_Date", "expression": "`Daily_Event_Date`" },
                      { "name": "App_Version", "expression": "`App_Version`" },
                      { "name": "Active_Devices", "expression": "`Active_Devices`" },
                      { "name": "Percentage_Active_Devices", "expression": "`Percentage_Active_Devices`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "line",
                "encodings": {
                  "x": {
                    "fieldName": "Daily_Event_Date",
                    "displayName": "Daily Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "Percentage_Active_Devices",
                    "displayName": "% Active Devices",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fieldName": "App_Version",
                    "displayName": "App Version",
                    "scale": { "type": "categorical" }
                  }
                },
                "frame": {
                  "title": "HPX New Version Adoption",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }