{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "hpx_new_version_adoption_page",
      "displayName": "HPX New Version Adoption",
      "layout": [
        {
          "widget": {
            "name": "version_adoption_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Daily Event Date",
                      "expression": "`Daily Event Date`"
                    },
                    {
                      "name": "App Version",
                      "expression": "`App Version`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "% Active Devices",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER (PARTITION BY `Daily Event Date`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "Daily Event Date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "% Active Devices",
                  "displayName": "% Active Devices",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "App Version",
                  "displayName": "App Version",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "HPX New Version Adoption",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}