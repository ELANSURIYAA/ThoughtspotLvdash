{
  "datasets": [
    {
      "name": "hpx_homepage_summary_dataset",
      "displayName": "HPX Homepage Summary",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "home_tile_clicks_page",
      "displayName": "Home Tile Clicks",
      "layout": [
        {
          "widget": {
            "name": "home_tile_clicks_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_homepage_summary_dataset",
                  "fields": [
                    {
                      "name": "Home Tile",
                      "expression": "`Home Tile`"
                    },
                    {
                      "name": "Unique Tile Clicks",
                      "expression": "MEASURE(`Total Unique Tile Clicks`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "y": {
                  "fieldName": "Unique Tile Clicks",
                  "displayName": "Unique Tile Clicks",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Home Tile Clicks"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}