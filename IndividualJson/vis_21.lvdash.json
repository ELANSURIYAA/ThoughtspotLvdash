{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_appversion_page",
      "displayName": "% Active Devices by Appversion",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_appversion_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "App Version",
                      "expression": "`App Version`"
                    },
                    {
                      "name": "Active Devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "Total Active Devices",
                      "expression": "SUM(MEASURE(`Active Devices`)) OVER ()"
                    },
                    {
                      "name": "% Active Devices By Appversion",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER ()"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "App Version",
                  "displayName": "App Version",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "% Active Devices By Appversion",
                  "displayName": "% Active Devices By Appversion",
                  "scale": { "type": "quantitative" },
                  "format": {
                    "type": "percent",
                    "maximumFractionDigits": 1
                  }
                }
              },
              "frame": {
                "title": "% Active Devices by Appversion",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "Event Date Time",
          "displayName": "Event Date Time",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "defaultValues": [{
            "value": "2023-05-01T00:00:00.000Z",
            "operator": "GREATER_THAN_OR_EQUAL_TO"
          }]
        },
        {
          "fieldName": "App Version",
          "displayName": "App Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "excludedValues": ["not a store release", "other non-releases", null]
        },
        {
          "fieldName": "Os Version",
          "displayName": "Os Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false,
          "excludedValues": [null]
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}