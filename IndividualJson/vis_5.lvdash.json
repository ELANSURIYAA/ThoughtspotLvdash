{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "top_screen_views_page",
      "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
      "layout": [
        {
          "widget": {
            "name": "top_screen_views_stacked_bar",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "monthly_event_date",
                      "expression": "`Monthly Event Date`"
                    },
                    {
                      "name": "screen",
                      "expression": "`Screen`"
                    },
                    {
                      "name": "unique_screen_views",
                      "expression": "MEASURE(`Unique Screen Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "monthly_event_date",
                  "displayName": "Monthly Event Date",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "unique_screen_views",
                  "displayName": "Unique Screen Views",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "screen",
                  "displayName": "Screen",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "title": "Top Unique Screen Views (excl. Home/Welcome)",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}