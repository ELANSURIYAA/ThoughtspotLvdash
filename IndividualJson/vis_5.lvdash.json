{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "top_unique_screen_views_page",
      "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
      "layout": [
        {
          "widget": {
            "name": "top_unique_screen_views_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Monthly Event Date",
                      "expression": "`Monthly Event Date`"
                    },
                    {
                      "name": "Screen",
                      "expression": "`Screen`"
                    },
                    {
                      "name": "Unique Screen Views",
                      "expression": "MEASURE(`Unique Screen Views`)"
                    }
                  ],
                  "filters": [
                    {
                      "name": "screen_filter",
                      "expression": "`Screen` != 'home' AND `Screen` != 'welcome' AND `Screen` != 'welcomecontainer' AND `Screen` != 'welcome-core' AND `Screen` != 'support'"
                    },
                    {
                      "name": "ui_action_filter",
                      "expression": "`Ui Action` = 'screendisplayed'"
                    },
                    {
                      "name": "date_filter",
                      "expression": "`Event Date Time` >= '2023-05-01'"
                    }
                  ],
                  "limit": 10,
                  "orderBy": [
                    {
                      "fieldName": "Unique Screen Views",
                      "direction": "DESC"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "Monthly Event Date",
                  "displayName": "Monthly Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "Unique Screen Views",
                  "displayName": "Unique Screen Views",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "Screen",
                  "displayName": "Screen",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Top Unique Screen Views (excl. Home/Welcome)",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}