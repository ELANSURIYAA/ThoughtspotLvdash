{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "Top Unique Screen Views Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "top_unique_screen_views_page",
      "displayName": "Top Unique Screen Views",
      "layout": [
        {
          "widget": {
            "name": "top_unique_screen_views_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    {
                      "name": "Daily Event Date",
                      "expression": "DATE_TRUNC('DAY', `Event Date Time`)"
                    },
                    {
                      "name": "Screen",
                      "expression": "`Screen`"
                    },
                    {
                      "name": "Unique Screen Views",
                      "expression": "MEASURE(`Unique Screen Views`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "Daily Event Date",
                  "displayName": "Daily Event Date",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "Unique Screen Views",
                  "displayName": "Unique Screen Views",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "Screen",
                  "displayName": "Screen",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Top Unique Screen Views (excl. Home/Welcome)"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "Event Date Time",
          "displayName": "Event Date Time",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "Screen",
          "displayName": "Screen",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}