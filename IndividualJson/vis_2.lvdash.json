{
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "new_devices_page",
      "displayName": "New Devices",
      "layout": [
        {
          "widget": {
            "name": "new_devices_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "Daily Event Date",
                      "expression": "`Daily Event Date`"
                    },
                    {
                      "name": "New Devices",
                      "expression": "MEASURE(`New Devices`)"
                    },
                    {
                      "name": "Cumulative New Devices",
                      "expression": "SUM(MEASURE(`New Devices`)) OVER (ORDER BY `Daily Event Date` ROWS UNBOUNDED PRECEDING)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "Daily Event Date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "New Devices",
                      "displayName": "New Devices"
                    },
                    {
                      "fieldName": "Cumulative New Devices",
                      "displayName": "Cumulative New Devices"
                    }
                  ],
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "New Devices and Cumulative New Devices Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}