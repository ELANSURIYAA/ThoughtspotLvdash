{
    "datasets": [
      {
        "name": "dataset_11",
        "displayName": "HPX CURR Trendline Dataset",
        "asset_name": "workspace.default.mv_hpx_curr",
        "queryLines": [
          "SELECT DATE_TRUNC('WEEK', Week_Active) AS Weekly_Active, AVG(CURR) AS Average_CURR FROM HPX_CURR GROUP BY DATE_TRUNC('WEEK', Week_Active) ORDER BY Weekly_Active"
        ]
      }
    ],
    "pages": [
      {
        "name": "page_11",
        "displayName": "HPX CURR Trendline Visualization",
        "layout": [
          {
            "widget": {
              "name": "widget_11",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_11",
                    "fields": [
                      { "name": "Weekly_Active", "expression": "`Weekly_Active`" },
                      { "name": "Average_CURR", "expression": "`Average_CURR`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "line",
                "encodings": {
                  "x": {
                    "fieldName": "Weekly_Active",
                    "displayName": "Week Active",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "Average_CURR",
                    "displayName": "Average CURR",
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "HPX CURR Trendline",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }