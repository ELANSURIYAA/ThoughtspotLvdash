{
  "datasets": [
    {
      "name": "dataset_viz_11",
      "displayName": "HPX CURR Trendline Dataset",
      "asset_name": "workspace.default.mv_hpx_curr"
    }
  ],
  "pages": [
    {
      "name": "page_viz_11",
      "displayName": "HPX CURR - Trendline",
      "layout": [
        {
          "widget": {
            "name": "hpx_curr_trendline_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_viz_11",
                  "fields": [
                    {
                      "name": "week_active",
                      "expression": "DATE_TRUNC('WEEK', `Week Active`)"
                    },
                    {
                      "name": "curr",
                      "expression": "AVG(MEASURE(`Curr`))"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "week_active",
                  "displayName": "Week Active",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "curr",
                  "displayName": "CURR",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "showTitle": true,
                "title": "HPX CURR - Trendline"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}