{
    "datasets": [
      {
        "name": "dataset_4",
        "displayName": "Total Clicks by Control Dataset",
        "asset_name": "workspace.default.hpx_events_model"
      }
    ],
    "pages": [
      {
        "name": "page_4",
        "displayName": "Total Clicks by Control Overview",
        "layout": [
          {
            "widget": {
              "name": "stacked_column_chart_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_4",
                    "fields": [
                      { "name": "Daily Event Date", "expression": "DATE_TRUNC('DAY', `Event Date Time`)" },
                      { "name": "Total Clicks", "expression": "MEASURE(`Total Clicks`)" },
                      { "name": "Control", "expression": "`Control`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "bar",
                "encodings": {
                  "x": {
                    "fieldName": "Daily Event Date",
                    "displayName": "Daily Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "Total Clicks",
                    "displayName": "Total Clicks",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fieldName": "Control",
                    "displayName": "Control",
                    "scale": { "type": "categorical" }
                  }
                },
                "frame": {
                  "title": "Total Clicks by Control Over Time",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }