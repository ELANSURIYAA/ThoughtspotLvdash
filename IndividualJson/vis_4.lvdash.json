{
  "datasets": [
    {
      "name": "dataset_4",
      "displayName": "Top On Press Events Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_4",
      "displayName": "Top On Press Events",
      "layout": [
        {
          "widget": {
            "name": "widget_4",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_4",
                  "fields": [
                    {
                      "name": "control",
                      "expression": "control"
                    },
                    {
                      "name": "total_clicks",
                      "expression": "SUM(clicks)"
                    },
                    {
                      "name": "daily_event_date",
                      "expression": "DATE_TRUNC('DAY', event_date_time)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "total_clicks",
                  "displayName": "Total Clicks",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "control",
                  "displayName": "Control"
                }
              },
              "frame": {
                "title": "Top On Press Events",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}