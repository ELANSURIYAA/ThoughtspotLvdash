{
    "datasets": [
      {
        "name": "dataset_17",
        "displayName": "Home Tile Interactions CTR Dataset",
        "asset_name": "workspace.hpx_events_model_homepage_summary.metric_view"
      }
    ],
    "pages": [
      {
        "name": "page_17",
        "displayName": "Home Tile Interactions CTR",
        "layout": [
          {
            "widget": {
              "name": "widget_17",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_17",
                    "fields": [
                      { "name": "home_tile", "expression": "`Home Tile`" },
                      { "name": "f_ctr", "expression": "MEASURE(`f_ctr`)" },
                      { "name": "unique_tile_views", "expression": "MEASURE(`Total Unique Tile Views`)" },
                      { "name": "unique_tile_clicks", "expression": "MEASURE(`Total Unique Tile Clicks`)" },
                      { "name": "unique_interstitial_views", "expression": "MEASURE(`Total Unique Interstitial Views`)" },
                      { "name": "unique_interstitial_primary_clicks", "expression": "MEASURE(`Total Unique Interstitial Primary Button Clicks`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "y": {
                    "fields": [
                      {
                        "fieldName": "f_ctr",
                        "displayName": "% CTR"
                      },
                      {
                        "fieldName": "unique_tile_views",
                        "displayName": "Unique Tile Views"
                      },
                      {
                        "fieldName": "unique_tile_clicks",
                        "displayName": "Unique Tile Clicks"
                      },
                      {
                        "fieldName": "unique_interstitial_views",
                        "displayName": "Unique Interstitial Views"
                      },
                      {
                        "fieldName": "unique_interstitial_primary_clicks",
                        "displayName": "Unique Interstitial Primary Button Clicks"
                      }
                    ],
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "Home Tile Interactions CTR",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }