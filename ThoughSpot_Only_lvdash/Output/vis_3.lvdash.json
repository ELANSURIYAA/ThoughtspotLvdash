{
  "datasets": [
    {
      "name": "dataset_3",
      "displayName": "Top 10 Countries Dataset",
      "asset_name": "workspace.default.mv_hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_3",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "widget_3",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_3",
                  "fields": [
                    {
                      "name": "geo_country_code",
                      "expression": "geo_country_code"
                    },
                    {
                      "name": "new_devices",
                      "expression": "SUM(new_devices)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 1,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "new_devices",
                  "displayName": "New Devices"
                },
                "color": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country"
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}