{
  "datasets": [
    {
      "name": "dataset_11",
      "displayName": "HPX CURR Trendline Dataset",
      "asset_name": "workspace.default.mv_hpx_curr"
    }
  ],
  "pages": [
    {
      "name": "page_11",
      "displayName": "HPX CURR - Trendline",
      "layout": [
        {
          "widget": {
            "name": "widget_11",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_11",
                  "fields": [
                    {
                      "name": "weekly_week_active",
                      "expression": "DATE_TRUNC('WEEK', week_active)"
                    },
                    {
                      "name": "curr",
                      "expression": "AVG(curr)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "weekly_week_active",
                  "displayName": "Week Active",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "curr",
                  "displayName": "CURR",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "HPX CURR - Trendline",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}