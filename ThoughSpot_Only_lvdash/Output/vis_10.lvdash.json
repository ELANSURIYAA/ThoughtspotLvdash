{
    "datasets": [
      {
        "name": "dataset_10",
        "displayName": "Total Consented Devices Dataset",
        "asset_name": "workspace.hpx_events_model.metricview"
      }
    ],
    "pages": [
      {
        "name": "page_10",
        "displayName": "Total Consented Devices",
        "layout": [
          {
            "widget": {
              "name": "widget_10",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_10",
                    "fields": [
                      { "name": "Active_Devices", "expression": "MEASURE(`Active Devices`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "counter",
                "encodings": {
                  "y": {
                    "fieldName": "Active_Devices",
                    "displayName": "Active Devices",
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "Total Consented Devices",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }