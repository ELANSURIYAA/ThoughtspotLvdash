{
  "datasets": [
    {
      "name": "dataset_9",
      "displayName": "HPX Retention Dataset",
      "asset_name": "workspace.default.mv_hpx_retention"
    }
  ],
  "pages": [
    {
      "name": "page_9",
      "displayName": "HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "widget_9",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_9",
                  "fields": [
                    {
                      "name": "monthly_first_device_event_date_time",
                      "expression": "DATE_TRUNC('MONTH', first_device_event_date_time)"
                    },
                    {
                      "name": "active_devices",
                      "expression": "COUNT(DISTINCT sys_serial_number)"
                    },
                    {
                      "name": "percent_devices_returning",
                      "expression": "AVG(percent_devices_returning)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "monthly_first_device_event_date_time",
                  "displayName": "Monthly First Device Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "active_devices",
                      "displayName": "Active Devices"
                    },
                    {
                      "fieldName": "percent_devices_returning",
                      "displayName": "% Devices Returning"
                    }
                  ],
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "HPX Retention",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}