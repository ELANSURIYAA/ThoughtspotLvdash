{
  "datasets": [
    {
      "name": "dataset_15",
      "displayName": "Home Tile Views Dataset",
      "asset_name": "workspace.default.mv_hpx_homepage_summary"
    }
  ],
  "pages": [
    {
      "name": "page_15",
      "displayName": "Home Tile Views",
      "layout": [
        {
          "widget": {
            "name": "widget_15",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_15",
                  "fields": [
                    {
                      "name": "home_tile",
                      "expression": "home_tile"
                    },
                    {
                      "name": "unique_tile_views",
                      "expression": "SUM(unique_tile_views)"
                    },
                    {
                      "name": "unique_tile_clicks",
                      "expression": "SUM(unique_tile_clicks)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "home_tile",
                  "displayName": "Home Tile",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "unique_tile_views",
                  "displayName": "Unique Tile Views",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "unique_tile_clicks",
                  "displayName": "Unique Tile Clicks",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Home Tile Views",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}