{
    "datasets": [
      {
        "name": "dataset_5",
        "displayName": "Top Unique Screen Views Dataset",
        "asset_name": "workspace.hpx_events_model.metricview"
      }
    ],
    "pages": [
      {
        "name": "page_5",
        "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
        "layout": [
          {
            "widget": {
              "name": "widget_5",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_5",
                    "fields": [
                      { "name": "Monthly_Event_Date", "expression": "DATE_TRUNC('DAY', `Event Date Time`)" },
                      { "name": "Screen", "expression": "`Screen`" },
                      { "name": "Unique_Screen_Views", "expression": "MEASURE(`Unique Screen Views`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "bar",
                "encodings": {
                  "x": {
                    "fieldName": "Monthly_Event_Date",
                    "displayName": "Monthly Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "Unique_Screen_Views",
                    "displayName": "Unique Screen Views",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fieldName": "Screen",
                    "displayName": "Screen"
                  }
                },
                "frame": {
                  "title": "Top Unique Screen Views (excl. Home/Welcome)",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }