{
    "datasets": [
      {
        "name": "dataset_1",
        "displayName": "Active Devices Dataset",
        "asset_name": "workspace.hpx_events_model.metric_view"
      }
    ],
    "pages": [
      {
        "name": "page_1",
        "displayName": "Active Devices",
        "layout": [
          {
            "widget": {
              "name": "widget_1",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_1",
                    "fields": [
                      { "name": "daily_event_date", "expression": "DATE_TRUNC('DAY', `Event Date Time`)" },
                      { "name": "active_devices", "expression": "MEASURE(`Active Devices`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "line",
                "encodings": {
                  "x": {
                    "fieldName": "daily_event_date",
                    "displayName": "Daily Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "active_devices",
                    "displayName": "Active Devices",
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "Active Devices Over Time",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }