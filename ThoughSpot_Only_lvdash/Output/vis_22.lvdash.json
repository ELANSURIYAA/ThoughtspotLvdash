{
    "datasets": [
      {
        "name": "dataset_22",
        "displayName": "HPX New Version Adoption Dataset",
        "asset_name": "workspace.hpx_events_model.metricview"
      }
    ],
    "pages": [
      {
        "name": "page_22",
        "displayName": "HPX New Version Adoption",
        "layout": [
          {
            "widget": {
              "name": "widget_22",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_22",
                    "fields": [
                      { "name": "Daily_Event_Date", "expression": "DATE_TRUNC('DAY', `Event Date Time`)" },
                      { "name": "App_Version", "expression": "`App Version`" },
                      { "name": "Active_Devices", "expression": "MEASURE(`Active Devices`)" },
                      { "name": "Active_Devices_Percentage", "expression": "MEASURE(`% Active Devices`)" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "line",
                "encodings": {
                  "x": {
                    "fieldName": "Daily_Event_Date",
                    "displayName": "Daily Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fieldName": "Active_Devices_Percentage",
                    "displayName": "% Active Devices",
                    "scale": { "type": "quantitative" }
                  },
                  "color": {
                    "fieldName": "App_Version",
                    "displayName": "App Version"
                  }
                },
                "frame": {
                  "title": "HPX New Version Adoption",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }