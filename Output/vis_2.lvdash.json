{
    "datasets": [
      {
        "name": "dataset_2",
        "displayName": "HPX Events Model",
        "asset_name": "workspace.default.hpx_events_model",
        "queryLines": [
          "SELECT DATE_TRUNC('DAY', `Event Date Time`) AS `Daily Event Date`, MEASURE(`New Devices`) AS `New Devices`, MEASURE(SUM(`New Devices`)) OVER (ORDER BY DATE_TRUNC('DAY', `Event Date Time`) ROWS UNBOUNDED PRECEDING) AS `Cumulative New Devices` FROM workspace.default.hpx_events_model GROUP BY DATE_TRUNC('DAY', `Event Date Time`) ORDER BY `Daily Event Date`"
        ]
      }
    ],
    "pages": [
      {
        "name": "page_2",
        "displayName": "New Devices",
        "layout": [
          {
            "widget": {
              "name": "line_chart_widget",
              "queries": [
                {
                  "name": "main_query",
                  "query": {
                    "datasetName": "dataset_2",
                    "fields": [
                      { "name": "Daily Event Date", "expression": "`Daily Event Date`" },
                      { "name": "New Devices", "expression": "`New Devices`" },
                      { "name": "Cumulative New Devices", "expression": "`Cumulative New Devices`" }
                    ],
                    "disaggregated": false
                  }
                }
              ],
              "spec": {
                "version": 3,
                "widgetType": "line",
                "encodings": {
                  "x": {
                    "fieldName": "Daily Event Date",
                    "displayName": "Daily Event Date",
                    "scale": { "type": "temporal" }
                  },
                  "y": {
                    "fields": [
                      {
                        "fieldName": "New Devices",
                        "displayName": "New Devices"
                      },
                      {
                        "fieldName": "Cumulative New Devices",
                        "displayName": "Cumulative New Devices"
                      }
                    ],
                    "scale": { "type": "quantitative" }
                  }
                },
                "frame": {
                  "title": "New and Cumulative Devices Over Time",
                  "showTitle": true
                }
              }
            },
            "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
          }
        ],
        "pageType": "PAGE_TYPE_CANVAS",
        "filters": []
      }
    ],
    "uiSettings": {
      "theme": {
        "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
      }
    }
  }